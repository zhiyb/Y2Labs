# Poly2 resistor
device = RES(
	     RLAYER=poly2 resistor;
	     Plus=poly2 wire, WIDTH;
	     Minus=poly2 wire, WIDTH;
	     MODEL=RPOLY2;
	     )

# RPOLYH resistor
device = RES(
            RLAYER=rpolyh resistor;
            Plus=poly2 wire, width;
            Minus=poly2 wire, width;
            MODEL=RPOLYH;
            )

# N Well resistor
device = RES(
            RLAYER=nwell resistor;
            Plus=n well wire, WIDTH;
            Minus=n well wire, WIDTH;
            MODEL=RNWELL;
            )

